write_conf(){
if [[ $3 == "" ]];then
echo "`$cat $aria2conf |$sed "/$1=/c $1=$2"`" >$aria2conf
elif [[ $3 != "" &&  $4 == "" ]];then
echo "`$cat $aria2conf |$sed "s/$3/XXXXX/g"|$sed "/$1=/c $1=$2"|$sed "s/XXXXX/$3/g"`" >$aria2conf
